{
	"name": "teleink",
	"version": "0.0.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"build": "tsc",
		"start": "npm run build && dist/cli.js",
		"pretest": "npm run build",
		"test": "xo && ava"
	},
	"files": [
		"dist/cli.js"
	],
	"dependencies": {
		"ink": "^3.2.0",
		"ink-text-input": "^4.0.2",
		"meow": "^9.0.0",
		"prompts": "^2.4.2",
		"react": "^17.0.2",
		"telegraf": "^4.6.0",
		"uuid": "^8.3.2"
	},
	"ava": {
		"typescript": {
			"extensions": [
				"tsx"
			],
			"rewritePaths": {
				"source/": "dist/"
			}
		}
	},
	"xo": {
		"extends": "xo-react",
		"rules": {
			"react/prop-types": "off"
		}
	},
	"devDependencies": {
		"@ava/typescript": "^3.0.1",
		"@sindresorhus/tsconfig": "^2.0.0",
		"@types/react": "^17.0.38",
		"ava": "^4.0.1",
		"chalk": "^4.1.2",
		"eslint-config-xo-react": "^0.26.0",
		"eslint-plugin-react": "^7.28.0",
		"eslint-plugin-react-hooks": "^4.3.0",
		"ink-testing-library": "^2.1.0",
		"prettier": "2.5.1",
		"typescript": "^4.5.5",
		"xo": "^0.47.0"
	}
}
